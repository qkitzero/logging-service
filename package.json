{
  "name": "@qkitzero/logging-service-openapi",
  "version": "0.0.0",
  "files": [
    "gen/",
    "README.npm.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qkitzero/logging-service.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "generate:openapi": "ts-node ./src/interface/registry.ts",
    "generate:auth:openapi": "swagger2openapi ./node_modules/@qkitzero/auth-service-openapi/gen/openapiv2/auth/v1/auth.swagger.json -o ./src/infrastructure/api/auth.openapi.json",
    "generate:auth:schema": "openapi-typescript ./src/infrastructure/api/auth.openapi.json -o ./src/infrastructure/api/auth.schema.ts",
    "generate:all": "npm run generate:auth:openapi && npm run generate:auth:schema && npm run generate:openapi",
    "prisma:generate": "prisma generate --schema=src/infrastructure/prisma/schema.prisma",
    "prisma:migrate:create": "prisma migrate dev --schema=src/infrastructure/prisma/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema=src/infrastructure/prisma/schema.prisma",
    "prisma:migrate:reset": "prisma migrate reset --schema=src/infrastructure/prisma/schema.prisma",
    "build": "rimraf dist && tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "jest",
    "test:cov": "jest --coverage",
    "coverage:open": "open coverage/lcov-report/index.html"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "@prisma/client": "^6.16.2",
    "express": "^5.2.0",
    "openapi-fetch": "^0.15.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@qkitzero/auth-service-openapi": "^1.4.9",
    "@tsconfig/node24": "^24.0.3",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@types/uuid": "^10.0.0",
    "baseline-browser-mapping": "^2.8.29",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jest": "^29.7.0",
    "openapi-typescript": "^7.10.1",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prisma": "^6.16.2",
    "rimraf": "^6.0.1",
    "swagger2openapi": "^7.0.8",
    "ts-jest": "^29.4.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.45.0"
  }
}
